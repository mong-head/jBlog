<%
	for(var i = 0; i < data.length ; i++){
		var vo = data[i];
%>
		<tr data-no="<%=vo.no %>">
			<td id="table-no"><%=data.length - i %></td>
			<td><%=vo.name %></td>
			<td><%=vo.count %></td>
			<td><%=vo.description %></td>
			<td><a href="" data-no="<%=vo.no %>">
				<img src="/jblog03/assets/images/delete.jpg" /></a>
			</td>
		</tr>
<%
	}
%>
